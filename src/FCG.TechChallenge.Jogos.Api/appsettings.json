{
  "ConnectionStrings": {
    "Postgres": "${POSTGRES_CONNECTION_STRING}"
  },
  "ServiceBus": {
    "ConnectionString": "${SERVICEBUS_CONNECTION_STRING}",
    "QueueName": "${SERVICEBUS_QUEUE_NAME}"
  },
  "Elasticsearch": {
    "Uri": "${ELASTICSEARCH_URI}",
    "ApiKey": "${ELASTICSEARCH_APIKEY}",
    "IndexName": "${ELASTICSEARCH_INDEXNAME}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}