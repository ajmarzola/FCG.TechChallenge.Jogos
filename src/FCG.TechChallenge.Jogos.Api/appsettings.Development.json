{
  "ConnectionStrings": {
    "Postgres": "${POSTGRES_CONNECTION_STRING}"
  },
  "ServiceBus": {
    "ConnectionString": "${SERVICEBUS_CONNECTION_STRING}",
    "QueueName": "${SERVICEBUS_QUEUE_NAME}"
  },
  "Elastic": {
    "CloudId": "${ELASTIC_CLOUDID}",
    "ApiKeyId": "${ELASTIC_APIKEYID}",
    "ApiKey": "${ELASTIC_APIKEY}",
    "Index": "${ELASTIC_INDEX}",
    "DisablePing": "${ELASTIC_DISABLEPING}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}